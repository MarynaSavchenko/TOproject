<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.40"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainOverviewController">
    <center>
            <VBox alignment="CENTER" prefHeight="222.0" prefWidth="301.0" BorderPane.alignment="CENTER">
                <opaqueInsets>
                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                </opaqueInsets>
                <children>
                    <Label text="Hello!">
                        <font>
                            <Font size="22.0"/>
                        </font>
                    </Label>
                    <Label text="What do you want to see?">
                        <font>
                            <Font size="18.0"/>
                        </font>
                    </Label>
                    <HBox alignment="CENTER" prefHeight="57.0" prefWidth="281.0" spacing="20.0">
                        <children>
                            <Button fx:id="goToSensorsButton" alignment="TOP_CENTER" maxWidth="1.7976931348623157E308"
                                    mnemonicParsing="false" onAction="#goToSensors" text="SENSORS"/>
                            <Button fx:id="goToTasksButton" alignment="TOP_CENTER" maxWidth="1.7976931348623157E308"
                                    mnemonicParsing="false" onAction="#goToTasks" text="TASKS"/>
                        </children>
                    </HBox>
                </children>
                <BorderPane.margin>
                    <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                </BorderPane.margin>
            </VBox>
    </center>
</BorderPane>